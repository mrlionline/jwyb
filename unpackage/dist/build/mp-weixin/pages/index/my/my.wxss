.my-container{position:fixed;bottom:0;display:flex;flex-direction:column;flex-wrap:wrap;width:100%;height:74.6%;background:#fff;z-index:1;border-top-left-radius:24px;border-top-right-radius:24px}.user-info{position:relative;left:50%;top:-80rpx;-webkit-transform:translateX(-50%);transform:translateX(-50%);width:240rpx;height:360rpx}.user-info .user-star-tag{position:absolute;top:110rpx;left:calc(100% + 16px);display:flex;align-items:center;height:20px;padding:0 10px;background:#fdf1e8;border-radius:12px}.user-info .user-star-tag text{font-size:12px;text-align:center;color:#f2731e;white-space:nowrap}.user-info .user-header{width:240rpx;height:240rpx;padding:20rpx;border-radius:100%;background-color:#fff}.user-info .user-header image{max-width:100%;max-height:100%;border-radius:100%}.user-info .user-name{margin:8px 0;height:28px;line-height:28px;font-size:20px;color:#444251;letter-spacing:0;text-align:center}.user-info .user-star{display:flex;align-items:center;justify-content:center;height:16px}.user-info .user-star image{width:14px;height:14px;margin-right:2px}.user-info .user-star image:last-child{margin-right:0}.btn-wrap{position:relative;display:flex;align-items:center;width:654rpx;height:112rpx;padding:0 48rpx 0 36rpx;margin:0 auto;border:1px solid #d7d9db;border-radius:8px}.btn-wrap .arrow-right{position:absolute;top:50%;-webkit-transform:translateY(-50%);transform:translateY(-50%);right:23.5px}.btn-wrap .btn-text{margin-left:36rpx}.btn-wrap-reset-pwd{margin-bottom:16px}