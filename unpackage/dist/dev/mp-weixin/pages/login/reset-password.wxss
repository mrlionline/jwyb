.container {
  position: fixed;
  bottom: 0;
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  width: 100%;
  height: 74.6%;
  background: #FFFFFF;
  z-index: 1;
  border-top-left-radius: 24px;
  border-top-right-radius: 24px;
}
.login-form {
  padding: 64rpx;
}
.login-form .input-wrap {
  display: flex;
  align-items: center;
  width: 622rpx;
  height: 108rpx;
  padding: 0 13px;
  margin-bottom: 16px;
  border-radius: 8px;
  border: 1px solid #D7D9DB;
}
.login-form .input-wrap .divider {
  width: 1px;
  height: 76rpx;
  background-color: #D7D9DB;
  margin-left: 13px;
  margin-right: 23px;
}
.login-form .reset-btn {
  display: inline-block;
  width: 622rpx;
  height: 108rpx;
  line-height: 108rpx;
  text-align: center;
  background: #567DF4;
  font-size: 18px;
  color: #FFFFFF;
  border-radius: 29px;
}
.login-form .reset-btn-disabled {
  background-color: #D7D9DB;
}

